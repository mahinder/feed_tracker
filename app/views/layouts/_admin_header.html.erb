<div id="header">
  <a href="/">
    <%= image_tag '/assets/main_logo.png', :style => "padding-top: 4px;" %>
  </a>
  <div class="welcome navlinks pull-right">
    <% if user_signed_in? %>
      <span class="welcome_greet_user" >
        Welcome, <%= current_user.user_name.capitalize %></span> 
            <%= link_to "Logout",destroy_user_session_path, :method => :delete%> 
            <%= link_to "News" , admin_news_index_path  if current_user && current_user.is_admin%>
    <%end%>
  </div>
</div>